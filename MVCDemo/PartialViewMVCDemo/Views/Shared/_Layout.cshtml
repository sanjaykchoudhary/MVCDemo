<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        @{
            var containerPartialView = Html.Partial("_MenuPartialView");
            containerPartialView = new MvcHtmlString(containerPartialView.ToString() + "<h2>This is container Partial view</h2>");
        }
        @containerPartialView
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        @{ Html.RenderPartial("_FooterPartialView");}
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
